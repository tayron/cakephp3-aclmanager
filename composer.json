{
    "name": "pedrovalmor/cakephp3-aclmanager",
    "type": "cakephp-plugin",
    "keywords": ["cakephp3", "plugin", "acl"],
    "homepage": "https://github.com/pedrovalmor/cakephp3-aclmanager",
    "description": "CakePHP3 ACL Manager",
    "authors": [
        {
            "name": "Pedro Valmor",
            "email": "pedrovalmor@gmail.com",
